{
  "name": "nfl-no-serve",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "d3": "^4.10.0",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-geo": "^1.6.4",
    "d3-scale": "^1.0.6",
